{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "remoteCache": {
    "enabled": false
  },
  "globalEnv": [
    "TURBO_REMOTE_CACHE_ENABLED"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": [
        "TURBO_REMOTE_CACHE_ENABLED"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
